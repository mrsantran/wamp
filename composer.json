{
    "name": "santran/wamp",
    "description": "A Ratchet Server built for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.1",
        "cboden/ratchet": "^0.4.1",
        "react/zmq": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "homepage": "https://github.com/mrsantran/wamp",
    "autoload": {
        "psr-4": {
            "Santran\\WAMP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Santran\\WAMP\\Providers\\LaravelRatchetServiceProvider"
            ]
        }
    }
}
