{
    "name": "santran/wamp",
    "description": "A Ratchet Server built for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6.3",
        "ext-zmq": "*",
        "cboden/ratchet": "^0.4.1",
        "graham-campbell/throttle": "^7.0",
        "illuminate/console": "^5.5",
        "illuminate/support": "^5.5",
        "react/zmq": "0.2.*|0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Santran\\WAMP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Santran\\WAMP\\Providers\\LaravelRatchetServiceProvider"
            ]
        }
    }
}
